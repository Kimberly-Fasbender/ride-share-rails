<%= form_with model: @trip_in_progress, url: {action: "complete", controller: "trips", id: @trip_in_progress.id}, method: :patch, form_class: "rate trip form" do |f| %>

  <%= f.submit "Rate Trip", class: "button"%>
  <%= f.number_field :rating, :required => true, in: 1..5, step: 1 , class: "rate" %>


<%end%>